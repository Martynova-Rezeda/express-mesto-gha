{
  "env": {
    "browser": false,
    "node": true,
    "commonjs": true,
    "es2021": true,
    "amd": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "no-console": "off",
    "no-unused-vars":"off"
}
}